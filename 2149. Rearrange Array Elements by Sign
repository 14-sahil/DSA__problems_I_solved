class Solution {
public:
    vector<int> rearrangeArray(vector<int>& nums) {
        int size=nums.size();
        vector<int>arr2(size);
        int pos=0;
        int neg=1;
        for(int i=0;i<size;i++){
            if(nums[i]>0)
            {
                arr2[pos]=nums[i];
                pos=pos+2;
            }
            else{
                arr2[neg]=nums[i];
                neg=neg+2;
        
        
               }
        }
        
        nums=arr2;
        return nums;
    }
    
};
